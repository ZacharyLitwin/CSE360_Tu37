               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Text>
            <Button fx:id="deleteBtn" layoutX="49.0" layoutY="381.0" mnemonicParsing="false" onAction="#deleteEntryBtn" text="Delete This Entry" />
            <Text layoutX="21.0" layoutY="428.0" strokeType="OUTSIDE" strokeWidth="0.0" text="3.c. Split the Current Effort Log Entry into two entries">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Text>
            <Text layoutX="380.0" layoutY="427.0" strokeType="OUTSIDE" strokeWidth="0.0" text="4. Proceed to the Effort Log Console">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Text>
            <Button fx:id="splitBtn" layoutX="49.0" layoutY="431.0" mnemonicParsing="false" onAction="#splitEntryBtn" text="Split This Entry into Two Entries" />
            <Button fx:id="toLogBtn" layoutX="387.0" layoutY="431.0" mnemonicParsing="false" onAction="#proceedToLogBtn" text="Proceed to the Effort Log Console" />
