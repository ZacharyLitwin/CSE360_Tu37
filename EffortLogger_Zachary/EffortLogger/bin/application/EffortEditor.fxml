               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Text>
            <ComboBox fx:id="entries" layoutX="49.0" layoutY="128.0" onAction="#entrySelected" prefHeight="25.0" prefWidth="528.0" />
