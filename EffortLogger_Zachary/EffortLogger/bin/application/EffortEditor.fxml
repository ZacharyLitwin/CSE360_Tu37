               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Text>
            <Text layoutX="34.0" layoutY="208.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Date: " />
            <Text layoutX="211.0" layoutY="208.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Start Time:" />
