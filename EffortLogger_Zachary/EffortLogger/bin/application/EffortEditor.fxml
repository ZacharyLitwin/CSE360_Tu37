               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Text>
            <ComboBox fx:id="categories" layoutX="34.0" layoutY="292.0" onAction="#categoryChanged" prefHeight="25.0" prefWidth="216.0" />
            <ComboBox fx:id="plans" layoutX="261.0" layoutY="292.0" onAction="#planChanged" prefHeight="25.0" prefWidth="313.0" />
            <ComboBox fx:id="deliverables" layoutX="261.0" layoutY="292.0" onAction="#deliverableChanged" prefHeight="25.0" prefWidth="313.0" />
            <ComboBox fx:id="interruptions" layoutX="261.0" layoutY="292.0" onAction="#interruptionChanged" prefHeight="25.0" prefWidth="313.0" />
            <ComboBox fx:id="defects" layoutX="261.0" layoutY="292.0" onAction="#defectChanged" prefHeight="25.0" prefWidth="313.0" />
            <ComboBox fx:id="others" layoutX="261.0" layoutY="292.0" onAction="#otherChanged" prefHeight="25.0" prefWidth="313.0" />
            
            <Text fx:id="attributesChangesSaved" layoutX="65.0" layoutY="354.0" strokeType="OUTSIDE" strokeWidth="0.0" text="These attributes have been saved." />
